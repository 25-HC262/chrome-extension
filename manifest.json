{
    "manifest_version": 3,
    "name": "Sign translater",
    "version": "1.0.0",
    "description": "Sign translater for sign language user",
    "content_scripts": [
        {
            "matches": ["https://meet.google.com/*"],
            "js": ["./src/main.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "scripting"
    ]
}